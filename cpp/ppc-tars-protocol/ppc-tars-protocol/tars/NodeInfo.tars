module ppctars
{
    struct NodeInfo
    {
        1 require vector<byte> nodeID;
        2 require string endPoint;
        3 optional vector<string> components;
    };
    struct GatewayNodeInfo
    {
        1 require string p2pNodeID;
        2 require string agency;
        3 optional vector<NodeInfo> nodeList;
        4 optional int statusSeq;
    };
};
